      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playcanvas/supersplat-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/playcanvas/supersplat-viewer/issues"
  },
  "files": [
    "dist",
    "public",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@playcanvas/eslint-config": "2.1.0",
    "@playcanvas/web-components": "0.2.10",
    "@rollup/plugin-node-resolve": "16.0.2",
    "@rollup/plugin-typescript": "12.1.4",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.1",
    "eslint": "9.37.0",
    "globals": "16.4.0",
    "playcanvas": "2.12.1",
    "postcss": "8.5.6",
    "publint": "0.3.14",
    "rollup": "4.52.4",
    "rollup-plugin-copy": "3.5.0",
