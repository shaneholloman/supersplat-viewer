  "homepage": "https://github.com/playcanvas/supersplat-viewer#readme",
  "description": "Viewer for https://superspl.at",
  "keywords": [
    "playcanvas",
    "ply",
    "gaussian",
    "splat",
    "viewer"
  ],
  "license": "MIT",
