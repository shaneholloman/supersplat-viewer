    "sass": "1.93.2",
    "serve": "14.2.5",
    "typescript": "5.9.3"
  },
  "scripts": {
    "build": "rollup -c",
    "develop": "concurrently --kill-others \"npm run watch\" \"npm run serve\"",
    "lint": "eslint src rollup.config.mjs eslint.config.mjs",
    "lint:fix": "eslint src rollup.config.mjs eslint.config.mjs --fix",
    "publint": "publint",
    "serve": "serve public -C",
